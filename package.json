{
  "name": "ccpa.party",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@primer/octicons-react": "^17.1.0",
    "csvtojson": "^2.0.10",
    "emoji-name-map": "^1.2.9",
    "imurmurhash": "^0.1.4",
    "js-untar": "^2.0.0",
    "luxon": "^2.3.2",
    "pako": "^2.0.4",
    "react": "^18.1.0",
    "react-arborist": "^1.0.3",
    "react-dom": "^18.1.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^6.3.0",
    "react-virtualized": "^9.22.3",
    "react-virtuoso": "^2.11.0",
    "remark-gfm": "^3.0.1",
    "unzipit": "^1.4.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "colorjs.io": "^0.0.5",
    "flow-bin": "^0.177.0",
    "flow-types": "^1.0.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "IMAGE_INLINE_SIZE_LIMIT=0 craco start",
    "build": "IMAGE_INLINE_SIZE_LIMIT=0 craco build",
    "test": "craco test --env=./src/jestEnv.js",
    "flow": "flow",
    "lint": "eslint src/",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-restricted-imports": [
        "error",
        {
          "patterns": [
            ".*"
          ]
        }
      ],
      "no-sequences": "off"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!react-markdown)/"
    ]
  }
}
