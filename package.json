{
  "name": "ccpa.party",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@primer/octicons-react": "^17.1.0",
    "csvtojson": "^2.0.10",
    "emoji-name-map": "^1.2.9",
    "imurmurhash": "^0.1.4",
    "js-untar": "^2.0.0",
    "luxon": "^2.3.2",
    "minimatch": "^5.1.0",
    "pako": "^2.0.4",
    "react": "^18.1.0",
    "react-arborist": "^1.0.3",
    "react-dom": "^18.1.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^6.3.0",
    "react-virtuoso": "^2.11.0",
    "remark-gfm": "^3.0.1",
    "unzipit": "^1.4.0"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^4.0.1",
    "@testing-library/react": "^13.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/imurmurhash": "^0.1.1",
    "@types/luxon": "^2.3.2",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^17.0.35",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/web": "^0.0.67",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.0",
    "colorjs.io": "^0.0.5",
    "eslint": "^8.16.0",
    "happy-dom": "^4.0.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^2.9.9",
    "vitest": "^0.12.9"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "postbuild": "./prerender",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src/",
    "deploy": "vercel deploy --prod"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "no-restricted-imports": [
        "error",
        {
          "patterns": [
            ".*"
          ]
        }
      ]
    }
  },
  "prettier": {
    "importOrder": [
      "^@src/([^.]*)$",
      "^@src/(.*)\\.css$",
      "^@src/fonts/(.*)$",
      "^@src/img/(.*)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
